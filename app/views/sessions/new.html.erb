<div class="body_cover">
  <div class="sessionnew">
    <h1>ログイン</h1>
    <%= form_for(:session, url: log_path) do |f| %>
      <div class="form-group">
        <%= f.email_field :email, class: "form-control", placeholder: "メールアドレス" %>
      </div>
      <div class="form-group">
        <%= f.password_field :password, class: "form-control", placeholder: "パスワード" %>
      </div>
      <%= f.submit "ログイン", class: "btn btn-warning" %>
    <% end %>
  </div>
</div>